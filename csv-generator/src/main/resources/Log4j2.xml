<?xml version="1.0" encoding="UTF-8"?>

<Configuration>
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %-5p %c{2} - %m%n"/>
		</Console>
	</Appenders>

	<Loggers>
		<Root level="INFO">
			<Appender-ref ref="console" />
		</Root>
	</Loggers>

</Configuration>